<body>
    <div id="wrap">
        <!-- 左侧菜单栏目块 -->
        <div class="leftMeun" id="leftMeun">
            <div id="logoDiv">
                <p id="logoP"><img id="logo" alt="小葫芦程远项目" src="~/MUBAN/images/logo.png"><span>小葫芦程远项目</span></p>
            </div>
            <div id="personInfor">
                <p id="userName">@ViewBag.Name</p>
                <p>
                    <a href="javascript:void(0)" onclick="out()" style="text-decoration:none">退出登录</a>
                </p>
            </div>
            <div class="meun-title">菜单导航</div>
            @foreach (var item in ViewBag.PerssionList as List<Jurisdictions>)
            {
                <div class="meun-item" href="#char" aria-controls="char" role="tab" data-toggle="tab" onclick="locaqq('@item.Url')"><img src="~/MUBAN/images/icon_chara_grey.png">@item.Name</div>
            }

            

            
        </div>
        <!-- 右侧具体内容栏目 -->
        <div id="rightContent">
            <a class="toggle-btn" id="nimei">
                <i class="glyphicon glyphicon-align-justify"></i>
            </a>
            <!-- Tab panes -->
            <div class="tab-content">
                <!-- 资源管理模块 -->
                <div role="tabpanel" class="tab-pane active" id="sour" style="width:100%;height:750px">

                    <img src="~/img/logoindex.gif" style="width:100%;height:100%" />

                </div>
                <!--页码块-->
                <footer class="footer"></footer>
            </div>
        </div>
    </div>

